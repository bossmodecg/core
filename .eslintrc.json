{
  "extends": [
    "airbnb"
  ],

  "plugins": [
  ],

  "env": {
    "jest": true
  },

  "rules": {
    "arrow-parens": [ "error", "always" ],
    "comma-dangle": [ "error", "never" ],
    "max-len": [ "warn", 120 ],
    "no-console": "off",
    "no-underscore-dangle": "off",
    "quotes": "off"
  }
}
